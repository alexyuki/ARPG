<Window x:Class="GetPngs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetPngs"
        mc:Ignorable="d"
        Title="提取png" Height="350" Width="525">
    <Grid>
        <Button x:Name="GetPngBtn_" Content="提取png" HorizontalAlignment="Left" Margin="364,255,0,0" VerticalAlignment="Top" Width="121" Height="37" Click="GetPngBtn"/>
        <TextBox x:Name="StartDirectoryName" HorizontalAlignment="Left" Height="23" Margin="55,95,0,0" TextWrapping="Wrap" Text="提取路径..." VerticalAlignment="Top" Width="317"/>
        <Button x:Name="SelectStartPath_" Content="选择提取路径" HorizontalAlignment="Left" Margin="55,36,0,0" VerticalAlignment="Top" Width="121" Height="37" Click="SelectStartPath" RenderTransformOrigin="0.501,-0.015"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="56,143,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="comboBox_SelectionChanged">
            <ComboBoxItem IsSelected="True">.png</ComboBoxItem>
            <ComboBoxItem >.jpg</ComboBoxItem>
            <ComboBoxItem>.tga</ComboBoxItem>
        </ComboBox>
    </Grid>
</Window>
